{
  "name": "@ymkz/eslint-config",
  "version": "1.6.0",
  "description": "ymkz's eslint config",
  "keywords": [
    "config",
    "eslint",
    "ymkz"
  ],
  "homepage": "https://github.com/ymkz/eslint-config",
  "bugs": {
    "url": "https://github.com/ymkz/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ymkz/eslint-config"
  },
  "license": "MIT",
  "author": "ymkz <ymkzly@gmail.com> (https://github.com/ymkz)",
  "main": "index.js",
  "files": [
    "import.js",
    "react.js",
    "typescript.js"
  ],
  "scripts": {
    "format": "prettier --write .",
    "lint": "prettier --check ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "post-checkout": "npm install",
      "post-merge": "npm install",
      "post-rewrite": "npm install"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,js,yml,json}": [
      "prettier --write"
    ]
  },
  "prettier": "@ymkz/prettier-config",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "3.3.0",
    "@typescript-eslint/parser": "3.3.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "eslint-plugin-unicorn": "20.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@ymkz/prettier-config": "1.2.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "prettier-plugin-packagejson": "2.2.5",
    "react": "16.13.1",
    "typescript": "3.9.5"
  },
  "peerDependencies": {
    "prettier": ">=2"
  },
  "publishConfig": {
    "access": "public"
  }
}
